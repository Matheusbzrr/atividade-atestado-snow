        <script><![CDATA[(function executeRule(current) {
  if (current.data_de_inicio && current.data_de_volta) {
    var ini = new GlideDateTime(current.data_de_inicio);
    var fim = new GlideDateTime(current.data_de_volta);
    var dias = Math.floor((fim.getNumericValue() - ini.getNumericValue()) / (1000*60*60*24));
    current.total_de_dias_ausente = dias;
  }
})(current);
]]></script>
